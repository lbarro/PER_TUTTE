\chapter{Exemple de fichier de configuration}
\begin{lstlisting}
0:PREP:SH:0,1;1,N;3,E:${DIR_SH_BAO}/ScpTraitDep.ksh:TE_ECH:Traitement des dependances de la table TE_ECH
1:PREP:SH:0,2;1,2;3,E:${DIR_SH_BAO}/SciOrdPurgeSA.ksh:TE_ECH_SA:Purge de la table SA.TE_ECH_SA
2:CTL:SH:0,3;1,3;3,E:${DIR_SH_BAO}/ScpFlagEntreeVide.ksh :${DIR_IN}/CDD_434_346_${dateChaGal}.txt 2 ${DIR_TMP}/SwitchEntreeVide_${nomFicCfg}:Controle du nombre de lignes du CDD Echeance SA.TE_ECH_SA
3:CTL:DS:0,4;1,4;3,E:JxRFRSK_TE_ECH_Ext:-param LPAR_DATETRAIT=${dateTraitement} -param LPAR_NOMFIC=CDD_434_346_${dateChaGal}.txt -param LPAR_NOMSCH=CDD_434_346.sch -param LPAR_NOMFICOUT=Echeance.ds -param LPAR_NMTAB=TE_ECH:Chargement du CDD Echeance
4:CTL:DS:0,5;1,5;3,E:JxRFRSK_TE_ECH_Ld:-param LPAR_NOMFIC=Echeance.ds -param LPAR_NMTAB=TE_ECH_SA:Load de la table SA.TE_ECH_SA
5:SA:SH:0,6;1,6;3,E:${DIR_SH_BAO}/ScpOrdIndexes.ksh:D SA.TE_ECH_SA#DMR.TE_ECH#:Drop des indexes
6:SA:SH:0,7;1,7;3,E:${DIR_SH_BAO}/ScpDetectDoublons.ksh:TE_ECH:Detection des doublons lors de l'alimentation de la table TE_ECH
7:SA:SQ:0,8;1,8;3,E:${DIR_DB2_DML}/Bascule_Echeance.sql::Bascule du contenu de la table TE_ECH_SA dans la table DMR.TE_ECH
8:SA:SH:0,9;1,9;3,E:${DIR_SH_BAO}/ScpOrdIndexes.ksh:C SA.TE_ECH_SA#DMR.TE_ECH#:Creation des indexes
9:SA:SQ:0,10;1,10;3,E:${DIR_DB2_DML}/Runstats_Echeance.sql::Runstats sur la table DMR.TE_ECH
10:CTL:SU:0,X;3,E:JxRFRSK_TE_ECH_Ext:IMP,CDD,1#REJ,Format_Ko_CDD,1#OK,Ds,1#:CDD CDD_434_346_${dateChaGal}.txt pour table SA.TE_ECH_SA
\end{lstlisting}
